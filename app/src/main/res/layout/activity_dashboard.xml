<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Dashboard"
    android:background="@drawable/omblue">




    <Button
        android:id="@+id/butmuscat"
        android:layout_width="126dp"
        android:layout_height="57dp"
        android:text="Muscat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.12" />

    <Button
        android:id="@+id/butMusandam"
        android:layout_width="126dp"
        android:layout_height="57dp"
        android:text="Musandam"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/butmuscat"
        app:layout_constraintVertical_bias="0.074" />

    <Button
        android:id="@+id/butBuraimi"
        android:layout_width="126dp"
        android:layout_height="57dp"
        android:text="Al Buraimi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/butMusandam"
        app:layout_constraintVertical_bias="0.094" />

    <Button
        android:id="@+id/butBNorth"
        android:layout_width="126dp"
        android:layout_height="57dp"
        android:text="Al Batinah North"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/butBuraimi"
        app:layout_constraintVertical_bias="0.137" />

    <Button
        android:id="@+id/butBSouth"
        android:layout_width="126dp"
        android:layout_height="57dp"
        android:text="Al Batinah South"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/butBNorth"
        app:layout_constraintVertical_bias="0.193" />

    <Button
        android:id="@+id/butADhahirah"
        android:layout_width="126dp"
        android:layout_height="57dp"
        android:text="A'Dhahirah"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/butBSouth"
        app:layout_constraintVertical_bias="0.387" />

    <Button
        android:id="@+id/butADakhiliya"
        android:layout_width="126dp"
        android:layout_height="57dp"
        android:text="A'Dakhiliya"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.885"
        app:layout_constraintStart_toEndOf="@+id/butMusandam"
        app:layout_constraintTop_toBottomOf="@+id/butmuscat"
        app:layout_constraintVertical_bias="0.074" />

    <Button
        android:id="@+id/butSNorth"
        android:layout_width="126dp"
        android:layout_height="57dp"
        android:text="A'Sharqiyah North"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toEndOf="@+id/butBuraimi"
        app:layout_constraintTop_toBottomOf="@+id/butADakhiliya"
        app:layout_constraintVertical_bias="0.091" />

    <Button
        android:id="@+id/butSSouth"
        android:layout_width="126dp"
        android:layout_height="57dp"
        android:text="A'Sharqiyah South"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.885"
        app:layout_constraintStart_toEndOf="@+id/butBNorth"
        app:layout_constraintTop_toBottomOf="@+id/butSNorth"
        app:layout_constraintVertical_bias="0.14" />

    <Button
        android:id="@+id/butAlwusta"
        android:layout_width="126dp"
        android:layout_height="57dp"
        android:text="Al Wusta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toEndOf="@+id/butBSouth"
        app:layout_constraintTop_toBottomOf="@+id/butSSouth"
        app:layout_constraintVertical_bias="0.194" />

    <Button
        android:id="@+id/butDhofar"
        android:layout_width="126dp"
        android:layout_height="57dp"
        android:text="Dhofar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toEndOf="@+id/butADhahirah"
        app:layout_constraintTop_toBottomOf="@+id/butAlwusta"
        app:layout_constraintVertical_bias="0.388" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Oman Governorates"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/butmuscat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_dashboard_logout"
        android:layout_width="109dp"
        android:layout_height="47dp"
        android:text="Signout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/butADhahirah"
        app:layout_constraintVertical_bias="0.542" />

</androidx.constraintlayout.widget.ConstraintLayout>

