<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ADakhiliya"
    android:background="@drawable/omblue">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="101dp"
        android:layout_height="156dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/dakhilya" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="139dp"
        android:layout_height="46dp"
        android:ems="10"
        android:inputType="text"
        android:text="ADakhiliya"
        android:textColor="#FAF6F6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.941"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.115" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="160dp"
        android:layout_height="228dp"
        android:layout_marginStart="16dp"
        android:text="Situated in the heart of Oman’s interior, A’Dakhiliya Governorate dominates the central plateau and is rich in history, thanks to its role as the land link to the interior regions of Oman from Muscat "
        android:textColor="#FAF8F8"
        android:textColorLink="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.075" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="162dp"
        android:layout_height="141dp"
        android:text="Governorate of A'Dakhiliya contains the following eight Wilayats: Nizwa ,Adam,Al Hamra,Bahla,Bid Bid
,Izki,Manah,Samail "
        android:textColor="#FFFCFC"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.197" />


    <Button
        android:id="@+id/buthome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:shadowColor="#BF6565"
        android:text="Home"
        android:textColor="#F4EEEE"
        android:textColorHighlight="#FB6060"
        android:textColorHint="#770F0F"
        android:textColorLink="#673AB7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.436"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.924"
        app:rippleColor="#985454"
        app:strokeColor="#F3EBEB" />


</androidx.constraintlayout.widget.ConstraintLayout>